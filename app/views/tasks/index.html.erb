
<div class="d-flex justify-content-between" style="width: 100%; position: fixed">


  <div class="serch-results">
    <h1>Search results:</h1>
    <ul>
      <%@tasks.each do |task|%>
        <li><%=task.title%></li>
      <%end%>
    </ul>
  </div>

  <div
    id="map"
    style="width: 100%;
    width: 500px;"
    data-markers="<%= @markers.to_json %>"
    data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
  ></div>
</div>
