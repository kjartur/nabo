<div class="profile-container ">
  <div class="row">
    <div class="col-md-6 img">
      <div class="profilephotobig">
      <%= cl_image_tag(@user.avatar) %>
      </div>
      <br>
      <h1><%= @user.first_name %></h1>
      <h5><%= @user.city %></h5>
    </div>
    <div class="col-md-6 details">
      <p>
      <%= @user.bio %>
      </p>
       <blockquote>


        <cite title="Quote"><%= @user.quote %><i class="icon-map-marker"></i></cite>
      </blockquote>
    </div>
  </div>
</div>
</div>
